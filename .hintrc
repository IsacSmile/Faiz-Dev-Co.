{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "off",
      {
        "link-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/aria": [
      "default",
      {
        "aria-required-children": "off",
        "aria-progressbar-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "listitem": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-overflow-scrolling",
          "scrollbar-width",
          "scrollbar-width: none",
          "-webkit-text-size-adjust",
          "scrollbar-color"
        ]
      }
    ],
    "disown-opener": "off",
    "meta-viewport": "off",
    "meta-charset-utf-8": "off"
  }
}